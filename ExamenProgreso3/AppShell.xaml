<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ExamenProgreso3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ExamenProgreso3"
    Shell.FlyoutBehavior="Disabled"
    Title="ExamenProgreso3">
    <TabBar>
        <!-- Definir cada tab con su respectiva página -->
        <Tab Title="Página 1" Icon="icon1.png">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
        </Tab>
        <Tab Title="Página 2" Icon="icon2.png">
            <ShellContent ContentTemplate="{DataTemplate local:PeliculasGuardadasPage}" />
        </Tab>
    </TabBar>
    

   
</Shell>
